#
# A mixture of fecl and DES
#

# All the atoms from diferent molecules will be separated at least 2.0
# Anstroms at the solution.

tolerance 2.0

# The file type of input and output files is PDB

filetype pdb

# The name of the output file

output FeTFSI.pdb

# molality: 0.1. 


structure pdb_files/Fe_ideal.pdb
  number 30
  inside box 0. 0. 0. 90 90 90
end structure

structure pdb_files/tfsi.pdb 
  number 60 
  inside box 0. 0. 0. 90 90 90 
end structure

structure pdb_files/tip3p.pdb
  number 16653
  inside box 0. 0. 0. 90 90 90
end structure

